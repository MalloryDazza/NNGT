# .coveragerc file to configure coverage report

[run]
branch = True
source = nngt/
omit = nngt/testing

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    raise AttributeError
    raise InvalidArgument
    if __name__ == .__main__.:
include = nngt/*
omit = nngt/testing
